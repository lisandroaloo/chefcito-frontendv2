{
  "expo": {
    "name": "Chefcito",
    "slug": "chefcito",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chefcito-logo.png",
    "scheme": "nombredetuapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.lisandrooalo.chefcitofrontendv2",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d437c95b-2977-40f0-961c-740f9f9d4c2b"
      }
    }
  }
}
